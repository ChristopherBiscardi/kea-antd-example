    "babel-plugin-emotion": "^8.0.12",
    "babel-plugin-import": "^1.6.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.1",
    "react-app-rewired": "^1.3.6"
  }
