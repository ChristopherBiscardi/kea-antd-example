  "private": true,
  "dependencies": {
    "antd": "^3.0.0",
    "emotion": "^8.0.12",
    "kea": "^0.27.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-emotion": "^8.0.12",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-localstorage": "^0.4.1",
    "reselect": "^3.0.1"
